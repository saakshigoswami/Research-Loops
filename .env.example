# WalletConnect (required for wallet connection – RainbowKit)
# Get from: https://cloud.walletconnect.com/
VITE_WALLETCONNECT_PROJECT_ID=your_walletconnect_project_id

# Supabase (required for studies, enrollments)
# Get these from: Supabase Dashboard → Settings → API
VITE_SUPABASE_URL=https://YOUR_PROJECT_REF.supabase.co
VITE_SUPABASE_ANON_KEY=your_anon_public_key_here

# Pinata (optional – for IPFS study metadata)
# Get JWT from: Pinata Dashboard → API Keys → New Key (enable pinJSONToIPFS)
# Without this, studies are created with ipfs_cid = null; metadata is not uploaded.
VITE_PINATA_JWT=your_pinata_jwt_here
